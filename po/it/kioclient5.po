#: kioclient.cpp:65
#, kde-format
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Sintassi:\n"

#: kioclient.cpp:66
#, kde-format
msgid ""
"  kioclient5 openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kioclient5 openProperties «url»\n"
"            # Apre un menu delle proprietà\n"
"\n"

#: kioclient.cpp:68
#, kde-format
msgid ""
"  kioclient5 exec 'url' ['mimetype']\n"
"            # Tries to open the document pointed to by 'url', in the "
"application\n"
"            #   associated with it in KDE. You may omit 'mimetype'.\n"
"            #   In this case the mimetype is determined\n"
"            #   automatically. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
"            #   'url' can be an executable, too.\n"
msgstr ""
"  kioclient5 exec «url» [«tipo MIME»]\n"
"            # Cerca di aprire il documento puntato dall'«url» "
"nell'applicazione\n"
"            #   associata in KDE. Puoi omettere il «tipo MIME».\n"
"            #   In questo caso il tipo di file viene determinato\n"
"            #   automaticamente. Ovviamente l'«url» può essere l'URL\n"
"            #   di un documento, oppure un file *.desktop.\n"
"            #   L'«url» può anche essere un file eseguibile.\n"

#: kioclient.cpp:75
#, kde-format
msgid ""
"  kioclient5 move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kioclient5 move «sorgente» «destinazione»\n"
"            # Sposta l'URL «sorgente» in «destinazione».\n"
"            #   «sorgente» può essere un elenco di URL.\n"

#: kioclient.cpp:78
#, kde-format
msgid ""
"            #   'dest' may be \"trash:/\" to move the files\n"
"            #   to the trash.\n"
msgstr ""
"            #   «destinazione» può essere «trash:/» per\n"
"            #   cestinare i file.\n"

#: kioclient.cpp:80
#, kde-format
msgid ""
"            #   the short version kioclient5 mv\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   è disponibile anche la versione breve\n"
"            #   kioclient5 mv.\n"
"\n"

#: kioclient.cpp:82
#, kde-format
msgid ""
"  kioclient5 download ['src']\n"
"            # Copies the URL 'src' to a user-specified location'.\n"
"            #   'src' may be a list of URLs, if not present then\n"
"            #   a URL will be requested.\n"
"\n"
msgstr ""
"  kioclient5 download [«sorgente»]\n"
"            # Copia l'URL «sorgente» in una posizione specificata "
"dall'utente.\n"
"            #   «sorgente» potrebbe essere un elenco di URL, e se non ce n'è "
"uno\n"
"            #   ti verrà richiesto.\n"
"\n"

#: kioclient.cpp:86
#, kde-format
msgid ""
"  kioclient5 copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kioclient5 copy «sorgente» «destinazione»\n"
"            # Copia l'URL «sorgente» in «destinazione».\n"
"            #   «sorgente» potrebbe essere un elenco di URL.\n"

#: kioclient.cpp:89
#, kde-format
msgid ""
"            #   the short version kioclient5 cp\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   è disponibile anche la versione breve\n"
"            #   kioclient5 cp.\n"
"\n"

#: kioclient.cpp:91
#, kde-format
msgid ""
"  kioclient5 cat 'url'\n"
"            # Writes out the contents of 'url' to stdout\n"
"\n"
msgstr ""
"  kioclient5 cat «url»\n"
"            # Scrive i contenuti di un «url» sullo standard output\n"
"\n"

#: kioclient.cpp:93
#, kde-format
msgid ""
"  kioclient5 ls 'url'\n"
"            # Lists the contents of the directory 'url' to stdout\n"
"\n"
msgstr ""
"  kioclient5 ls «url»\n"
"            # Elenca i contenuti della cartella «url» sullo standard output\n"
"\n"

#: kioclient.cpp:95
#, kde-format
msgid ""
"  kioclient5 remove 'url'\n"
"            # Removes the URL\n"
"            #   'url' may be a list of URLs.\n"
msgstr ""
"  kioclient5 remove «URL»\n"
"            # Rimuove l'URL\n"
"            #   «URL» può essere un elenco di URL.\n"

#: kioclient.cpp:98
#, kde-format
msgid ""
"            #   the short version kioclient5 rm\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   è disponibile anche la versione breve\n"
"            #   kioclient5 rm.\n"
"\n"

#: kioclient.cpp:101
#, kde-format
msgid "*** Examples:\n"
msgstr "*** Esempi:\n"

#: kioclient.cpp:102
#, kde-format
msgid ""
"  kioclient5 exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kioclient5 exec file:/home/weis/data/test.html\n"
"             // Apre il file con l'applicazione predefinita\n"
"\n"

#: kioclient.cpp:104
#, kde-format
msgid ""
"  kioclient5 exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kioclient5 exec ftp://localhost/\n"
"             // Apre una nuova finestra con un URL\n"
"\n"

#: kioclient.cpp:106
#, kde-format
msgid ""
"  kioclient5 exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kioclient5 exec file:/root/Desktop/emacs.desktop\n"
"             // Avvia Emacs\n"
"\n"

#: kioclient.cpp:108
#, kde-format
msgid ""
"  kioclient5 exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kioclient5 exec .\n"
"             // Apre la cartella attuale. Molto comodo.\n"
"\n"

