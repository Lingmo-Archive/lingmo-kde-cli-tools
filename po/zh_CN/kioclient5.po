#: kioclient.cpp:65
#, kde-format
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"语法：\n"

#: kioclient.cpp:66
#, kde-format
msgid ""
"  kioclient5 openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kioclient5 openProperties 'URL'\n"
"            # 打开一个属性菜单\n"
"\n"

#: kioclient.cpp:68
#, kde-format
msgid ""
"  kioclient5 exec 'url' ['mimetype']\n"
"            # Tries to open the document pointed to by 'url', in the "
"application\n"
"            #   associated with it in KDE. You may omit 'mimetype'.\n"
"            #   In this case the mimetype is determined\n"
"            #   automatically. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
"            #   'url' can be an executable, too.\n"
msgstr ""
"  kioclient5 exec 'URL' ['MIME 类型']\n"
"            #   尝试用 KDE 关联的程序中打开 URL 指向的文档。\n"
"            #   MIME 类型参数可以省略。 \n"
"            #   如果省略 MIME 类型，它将被自动检测决定。不过 URL 必须\n"
"            #   被明确指向一个文档或是 *.desktop 文件。\n"
"            #   指向可执行文件也是可以的。\n"

#: kioclient.cpp:75
#, kde-format
msgid ""
"  kioclient5 move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kioclient5 move '源位置' '目标位置'\n"
"            #   将 URL 从“源位置”移到“目标位置”。\n"
"            #   “源位置”也可以是 URL 列表。\n"

#: kioclient.cpp:78
#, kde-format
msgid ""
"            #   'dest' may be \"trash:/\" to move the files\n"
"            #   to the trash.\n"
msgstr "            #   “目标位置”可以是“trash:/”，以把文件移至回收站。\n"

#: kioclient.cpp:80
#, kde-format
msgid ""
"            #   the short version kioclient5 mv\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   kioclient5 mv 的简版\n"
"            #   同样可用。\n"
"\n"

#: kioclient.cpp:82
#, kde-format
msgid ""
"  kioclient5 download ['src']\n"
"            # Copies the URL 'src' to a user-specified location'.\n"
"            #   'src' may be a list of URLs, if not present then\n"
"            #   a URL will be requested.\n"
"\n"
msgstr ""
"  kioclient5 download ['源位置']\n"
"            #   将“源位置”所指的 URL 复制到用户指定的位置。\n"
"            #   “源位置”也可以是 URL 列表，至少需要指定一个。\n"
"\n"

#: kioclient.cpp:86
#, kde-format
msgid ""
"  kioclient5 copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kioclient5 copy '源位置' '目标位置'\n"
"            #   将“源位置”的 URL 复制到“目标位置”。\n"
"            #   “源位置”也可以是 URL 列表。\n"

#: kioclient.cpp:89
#, kde-format
msgid ""
"            #   the short version kioclient5 cp\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   kioclient5 cp 的简版\n"
"            #   同样可用。\n"
"\n"

#: kioclient.cpp:91
#, kde-format
msgid ""
"  kioclient5 cat 'url'\n"
"            # Writes out the contents of 'url' to stdout\n"
"\n"
msgstr ""
"  kioclient5 cat 'URL'\n"
"            # 将指定“URL”的内容写到标准输出\n"
"\n"

#: kioclient.cpp:93
#, kde-format
msgid ""
"  kioclient5 ls 'url'\n"
"            # Lists the contents of the directory 'url' to stdout\n"
"\n"
msgstr ""
"  kioclient5 ls 'URL'\n"
"            # 将指定“URL”的内容列表写到标准输出\n"
"\n"

#: kioclient.cpp:95
#, kde-format
msgid ""
"  kioclient5 remove 'url'\n"
"            # Removes the URL\n"
"            #   'url' may be a list of URLs.\n"
msgstr ""
"  kioclient5 remove 'URL 地址'\n"
"            # 移除指定的 URL\n"
"            # “URL 地址”也可以是一串列表。\n"

#: kioclient.cpp:98
#, kde-format
msgid ""
"            #   the short version kioclient5 rm\n"
"            #   is also available.\n"
"\n"
msgstr ""
"            #   kioclient5 的短版本号也是可用的。\n"
"\n"

#: kioclient.cpp:101
#, kde-format
msgid "*** Examples:\n"
msgstr "*** 示例：\n"

#: kioclient.cpp:102
#, kde-format
msgid ""
"  kioclient5 exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kioclient5 exec file:/home/weis/data/test.html\n"
"             // 以默认文件关联打开文件\n"
"\n"

#: kioclient.cpp:104
#, kde-format
msgid ""
"  kioclient5 exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kioclient5 exec ftp://localhost/\n"
"             // 在新窗口中打开 URL\n"
"\n"

#: kioclient.cpp:106
#, kde-format
msgid ""
"  kioclient5 exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kioclient5 exec file:/root/Desktop/emacs.desktop\n"
"             // 启动 Emacs 程序\n"
"\n"

#: kioclient.cpp:108
#, kde-format
msgid ""
"  kioclient5 exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kioclient5 exec .\n"
"             // 打开当前目录，非常方便。\n"
"\n"

